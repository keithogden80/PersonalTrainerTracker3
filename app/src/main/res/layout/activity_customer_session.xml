<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.bignerdranch.android.personaltrainertracker.CustomerSession">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Customer Sessions:"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/fragment3"
        app:layout_constraintRight_toLeftOf="@+id/textView2"
        android:layout_marginRight="8dp"/>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LastName,"
        tools:layout_editor_absoluteY="25dp"
        app:layout_constraintRight_toLeftOf="@+id/textView3"
        android:layout_marginRight="8dp"/>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="FirstName"
        tools:layout_editor_absoluteX="231dp"
        tools:layout_editor_absoluteY="25dp"/>

    <EditText
        android:id="@+id/editText2"
        android:layout_width="110dp"
        android:layout_height="39dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Completed?"
        android:layout_marginLeft="91dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="3dp"
        app:layout_constraintTop_toBottomOf="@+id/textView2"/>

    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="102dp"
        android:layout_height="28dp"
        android:onClick="onClickSign"
        android:text="Session: "
        app:layout_constraintLeft_toRightOf="@+id/imageView2"
        android:layout_marginLeft="35dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/checkBox4"
        app:layout_constraintRight_toLeftOf="@+id/editText9"
        android:layout_marginRight="8dp"/>

    <CheckBox
        android:id="@+id/checkBox2"
        android:layout_width="99dp"
        android:layout_height="24dp"
        android:onClick="onClickSign"
        android:text="Session: "
        android:layout_marginBottom="1dp"
        app:layout_constraintBottom_toTopOf="@+id/checkBox6"
        app:layout_constraintRight_toLeftOf="@+id/editText11"
        android:layout_marginRight="8dp"/>

    <CheckBox
        android:id="@+id/checkBox6"
        android:layout_width="106dp"
        android:layout_height="33dp"
        android:onClick="onClickSign"
        android:text="Session: "
        android:layout_marginBottom="0dp"
        app:layout_constraintBottom_toTopOf="@+id/checkBox5"
        app:layout_constraintRight_toLeftOf="@+id/editText12"
        android:layout_marginRight="8dp"/>

    <CheckBox
        android:id="@+id/checkBox5"
        android:layout_width="107dp"
        android:layout_height="32dp"
        android:onClick="onClickSign"
        android:text="Session: "
        tools:layout_editor_absoluteY="239dp"
        android:layout_marginLeft="117dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/editText13"
        android:layout_marginRight="8dp"/>

    <CheckBox
        android:id="@+id/checkBox4"
        android:layout_width="103dp"
        android:layout_height="26dp"
        android:onClick="onClickSign"
        android:text="Session: "
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/checkBox3"
        app:layout_constraintRight_toLeftOf="@+id/editText8"
        android:layout_marginRight="8dp"/>

    <CheckBox
        android:id="@+id/checkBox3"
        android:layout_width="103dp"
        android:layout_height="26dp"
        android:onClick="onClickSign"
        android:text="Session: "
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/checkBox2"
        app:layout_constraintRight_toLeftOf="@+id/editText10"
        android:layout_marginRight="8dp"/>

    <EditText
        android:id="@+id/editText4"
        android:layout_width="123dp"
        android:layout_height="39dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="# of Sessions"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/button2"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/checkBox5"
        app:layout_constraintHorizontal_bias="0.507"/>

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClickBuy"
        android:text="+ buy More Sessions"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/editText13"/>

    <EditText
        android:id="@+id/editText5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Credit Card:"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/radioButton"
        app:layout_constraintRight_toLeftOf="@+id/editText6"
        android:layout_marginRight="8dp"/>

    <EditText
        android:id="@+id/editText6"
        android:layout_width="71dp"
        android:layout_height="39dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Expires"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/radioButton2"/>

    <EditText
        android:id="@+id/editText7"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:hint="hint"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="-42dp"/>

    <android.support.design.widget.TextInputLayout
        android:layout_width="258dp"
        android:layout_height="43dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/editText5"
        android:id="@+id/textInputLayout3">

    </android.support.design.widget.TextInputLayout>

    <android.support.design.widget.TextInputLayout
        android:layout_width="92dp"
        android:layout_height="49dp"
        app:layout_constraintLeft_toRightOf="@+id/textInputLayout3"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/editText6"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent">

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="hint"/>
    </android.support.design.widget.TextInputLayout>

    <RadioButton
        android:id="@+id/radioButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Default Payment"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/editText4"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/radioButton2"
        android:layout_marginRight="8dp"/>

    <RadioButton
        android:id="@+id/radioButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Add new Payment Type"
        tools:layout_editor_absoluteX="150dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/button2"/>

    <EditText
        android:id="@+id/editText8"
        android:layout_width="97dp"
        android:layout_height="29dp"
        android:ems="10"
        android:inputType="date"
        android:layout_marginRight="63dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/editText9"/>

    <EditText
        android:id="@+id/editText10"
        android:layout_width="97dp"
        android:layout_height="29dp"
        android:ems="10"
        android:inputType="date"
        android:layout_marginTop="-4dp"
        app:layout_constraintTop_toBottomOf="@+id/editText8"
        android:layout_marginRight="63dp"
        app:layout_constraintRight_toRightOf="parent"/>

    <EditText
        android:id="@+id/editText11"
        android:layout_width="97dp"
        android:layout_height="29dp"
        android:ems="10"
        android:inputType="date"
        android:layout_marginTop="9dp"
        app:layout_constraintTop_toBottomOf="@+id/editText10"
        android:layout_marginRight="63dp"
        app:layout_constraintRight_toRightOf="parent"/>

    <EditText
        android:id="@+id/editText12"
        android:layout_width="97dp"
        android:layout_height="29dp"
        android:ems="10"
        android:inputType="date"
        android:layout_marginTop="-5dp"
        app:layout_constraintTop_toBottomOf="@+id/editText11"
        android:layout_marginRight="63dp"
        app:layout_constraintRight_toRightOf="parent"/>

    <EditText
        android:id="@+id/editText13"
        android:layout_width="97dp"
        android:layout_height="29dp"
        android:ems="10"
        android:inputType="date"
        android:layout_marginTop="-8dp"
        app:layout_constraintTop_toBottomOf="@+id/editText12"
        android:layout_marginRight="63dp"
        app:layout_constraintRight_toRightOf="parent"/>

    <EditText
        android:id="@+id/editText9"
        android:layout_width="97dp"
        android:layout_height="29dp"
        android:ems="10"
        android:inputType="date"
        tools:layout_editor_absoluteY="77dp"
        android:layout_marginRight="63dp"
        app:layout_constraintRight_toRightOf="parent"/>

    <fragment
        android:id="@+id/fragment3"
        android:name="com.bignerdranch.android.personaltrainertracker.LoggedIn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout="@layout/fragment_logged_in"/>

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="75dp"
        android:layout_height="87dp"
        app:srcCompat="@mipmap/contact_icon"
        android:layout_marginTop="54dp"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"/>

</android.support.constraint.ConstraintLayout>
